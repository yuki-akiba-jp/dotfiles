{
  "[json]": {
    "editor.quickSuggestions": {
      "strings": true
    },
    "editor.suggest.insertMode": "replace"
  },
  "editor.tabCompletion": "on",
  "editor.suggestSelection": "first",
  "editor.fontLigatures": false,
  "editor.acceptSelectedSuggestion": true,
  "editor.minimap.enabled": false,
  "editor.guides.bracketPairs": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "editor.fontFamily": "Hack Nerd Font",
  "vscode-neovim.neovimInitVimPaths.darwin": "~/.config/nvim/init.lua",
  "C_Cpp.default.cppStandard": "c++20",
  "code-runner.clearPreviousOutput": true,
  "code-runner.runInTerminal": true,
  "code-runner.executorMap": {
    "cpp": "cd $dir && g++ -std=c++20 $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
    "python": "cd $dir && python3 -u $fileName"
  },
  "npm.enableScriptExplorer": true,
  "material-icon-theme.showWelcomeMessage": false,
  "python.pythonPath": "/usr/bin/python3",
  "python.showStartPage": false,
  "python.linting.lintOnSave": true,
  "python.linting.enabled": true,
  "python.linting.pylintEnabled": false,
  "python.linting.flake8Enabled": true,
  "python.linting.flake8Args": [
    "--ignore=W293, W504",
    "--max-line-length=150",
    "--max-complexity=20"
  ],
  "python.formatting.provider": "autopep8",
  "python.formatting.autopep8Args": [
    "--aggressive"
  ],
  "python.analysis.extraPaths": [
    "./core"
  ],
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "files.autoSaveDelay": 5000,
  "security.workspace.trust.untrustedFiles": "open",
  "python.defaultInterpreterPath": "/usr/local/bin/python3.9",
  "liveServer.settings.donotShowInfoMsg": true,
  "liveServer.settings.ChromeDebuggingAttachment": false,
  "terminal.integrated.fontFamily": "Hack Nerd Font",
  "vscode-neovim.neovimExecutablePaths.darwin": "/usr/local/bin/nvim",
  "settingsSync.ignoredExtensions": [],
  "files.autoSave": "afterDelay",
  "eslint.format.enable": true,
  "hediet.vscode-drawio.resizeImages": null,
  "plantuml.exportOutDir": "img",
  "plantuml.exportSubFolder": false,
  "editor.accessibilitySupport": "off",
  "editor.inlineSuggest.enabled": true,
  "editor.renderWhitespace": "all",
  "redhat.telemetry.enabled": true,
  "github.copilot.advanced": {},
  "github.copilot.enable": {
    "*": true
  },
  "extensions.experimental.affinity": {
    "asvetliakov.vscode-neovim": 1
  },
  "git.openRepositoryInParentFolders": "never",
  "github.copilot.editor.enableAutoCompletions": true,
  "vim.incsearch": true,
  "vim.useSystemClipboard": true,
  "vim.useCtrlKeys": true,
  "vim.hlsearch": true,
  "window.zoomLevel": 0,
  "tabnine.experimentalAutoImports": true,
  "workbench.iconTheme": "material-icon-theme",
  "workbench.colorTheme": "Dracula",
  "editor.cursorBlinking": "expand",
  "vscode-edge-devtools.mirrorEdits": true,
  "workbench.list.keyboardNavigation": "simple",
  "workbench.editor.enablePreview": false,
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "macros": {
    "openInVerticalSplit": [
      "workbench.action.splitEditor",
      "workbench.action.openEditorAtIndex1"
    ]
  },
  "terminal.integrated.env.osx": {
    "FIG_NEW_SESSION": "1"
  },
  "vscode-neovim.compositeKeys": {
    "jj": {
      "command": "vscode-neovim.escape"
    }
  }
}
