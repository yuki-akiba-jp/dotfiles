[include]
    path = ~/dotfiles/packages/git/.gituserinfo.config
[core]
	editor = vim -c \"set fenc=utf-8\"
	pager = less -F -X
[color]
	diff = auto
	status = auto
	branch = auto


[alias]
a  =  add
cm = commit
cmm = commit -m
st = status
br = branch
co = checkout
cb = checkout -b
ps = push


[init]
	defaultBranch = main
