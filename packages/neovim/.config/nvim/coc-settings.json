{
  "snippets.extends": {
    "cpp": ["c"],
    "html": ["jinja"],
    "typescript": ["javascript"]
  },

  "snippets.priority": 99999,
  "suggest.languageSourcePriority": 200,
  "suggest.floatConfig": {
    "border": true,
    "rounded": true
  },
  "snippets.userSnippetsDirectory": "~/dotfiles/packages/coc-snippets/snippets",
  "snippets.ultisnips.directories": [
    "UltiSnips",
    "~/dotfiles/packages/coc-snippets/snippets"
  ],
  "suggest.noselect": false,
  "suggest.triggerAfterInsertEnter": true,
  "suggest.snippetIndicator": " â–º",
  "suggest.selection": "recentlyUsedByPrefix",
  "suggest.enablePreview": true,
  "suggest.acceptSuggestionOnCommitCharacter": true,
  "suggest.removeDuplicateItems": true,
  "coc.preferences.formatOnSaveFiletypes": [
    "typescriptreact",
    "javascriptreact",
    "markdown",
    "css",
    "javascript",
    "typescript",
    "json",
    "scss",
    "yaml"
  ],
  "tsserver.formatOnType": true,
  "eslint.autoFixOnSave": true,
  "htmldjango.djlint.lintOnSave": true,
  "python.formatting.autopep8Path": "/usr/local/bin/autopep8"
}
